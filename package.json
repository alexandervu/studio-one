{
  "name": "studio-one",
  "version": "1.0.0-alpha.1",
  "description": "Studio One Vue 3 UI Components",
  "main": "dist/studio-one.umd.js",
  "module": "dist/studio-one.es.js",
  "exports": {
    ".": {
      "import": "./dist/studio-one.es.js",
      "require": "./dist/studio-one.umd.js"
    }
  },
  "unpkg": "dist/studio-one.umd.min.js",
  "scripts": {
    "dev": "vite --config vite-app.config.ts",
    "build": "vite build --config vite-app.config.ts",
    "build:lib": "vite build --config vite-lib.config.ts",
    "serve": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandervu/studio-one.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "node",
    "javascript",
    "typescript",
    "vue-components",
    "components",
    "ui"
  ],
  "author": {
    "name": "Alexander Vu",
    "email": "alexander.vu.tuyet@gmail.com",
    "url": "https://alexstudio.one/"
  },
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexandervu/studio-one/issues"
  },
  "homepage": "https://github.com/alexandervu/studio-one#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "workspaces": [
    "./packages/*"
  ],
  "files": [
    "dist/",
    "lib/types/"
  ],
  "types": "./lib/types/index.d.ts",
  "dependencies": {
    "vue": "^3.0.7",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.34",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.4",
    "cssnano": "^4.1.10",
    "postcss-atroot": "^0.1.3",
    "postcss-each": "^0.10.0",
    "postcss-extend-rule": "^3.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-property-lookup": "^2.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.0"
  }
}
