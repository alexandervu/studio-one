{
  "name": "studio-one",
  "version": "1.0.0-alpha.3",
  "description": "Studio One Vue 3 UI Components",
  "main": "dist/studio-one.umd.js",
  "browser": "dist/studio-one.esm.js",
  "module": "dist/studio-one.esm.js",
  "unpkg": "dist/studio-one.cjs.js",
  "types": "studio-one.d.ts",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/studio-one.es.js",
      "require": "./dist/studio-one.umd.js"
    }
  },
  "files": [
    "dist/*",
    "studio-one.d.ts",
    "lib/**/*.vue"
  ],
  "scripts": {
    "dev": "vite --config vite-app.config.ts",
    "build:storybook": "vite build --config vite-app.config.ts",
    "build:lib": "vite build --config vite-lib.config.ts",
    "serve": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandervu/studio-one.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "node",
    "javascript",
    "typescript",
    "vue-components",
    "components",
    "ui"
  ],
  "author": {
    "name": "Alexander Vu",
    "email": "alexander.vu.tuyet@gmail.com",
    "url": "https://alexstudio.one/"
  },
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexandervu/studio-one/issues"
  },
  "homepage": "https://github.com/alexandervu/studio-one#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "workspaces": [
    "./packages/*"
  ],
  "dependencies": {
    "@types/color-convert": "^2.0.0",
    "color-convert": "^2.0.1",
    "highlight.js": "^10.7.1",
    "highlightjs-line-numbers.js": "^2.8.0",
    "lodash": "^4.17.21",
    "vue": "^3.0.11",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.4",
    "cssnano": "^5.0.1",
    "postcss-atroot": "^0.1.3",
    "postcss-each": "^1.0.0",
    "postcss-extend-rule": "^3.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-property-lookup": "^3.0.0",
    "typescript": "^4.2.4",
    "vite": "^2.2.3"
  }
}
